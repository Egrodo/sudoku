{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "linebreak-style": 0,
        "react/no-unused-state": 0,
        "react/no-array-index-key": 0,
        "react/jsx-filename-extension": 0,
        "class-methods-use-this": 0,
        "no-plusplus": 0,
        "no-param-reassign": 0,
        "no-continue": 0
    }
}